<% include ../menu.html %><div class="container-fluid" style="margin-left: 15%;margin-top: -40%">
<div class="page-header"><h1> Car Rental Agents Online</h1></div>
    <div class="col-md-5">
        <% var keys=Object.keys(onlinecarrentalagents)  %>

        <table class="table" border="2">
            <thead class="thead-dark">
            <tr >
                <th scope="col">Total Online</th>
                <th scope="col"><p><%= size %></p></th>
            </tr>
            <tr>
                <th scope="col">UserName</th>
                <th scope="col">Phone</th>
                <th scope="col" >View Location</th>
            </tr>
            </thead>
            <tbody>
            <% keys.forEach(function(agent){ %>
                <tr onmouseover="{ viewcarrentalAgentLocation('<%= agent %>') }">
                    <td><%= onlinecarrentalagents[agent].agentName %></td>
                    <td><%= onlinecarrentalagents[agent].agentPhone %></td>
                    <td><button class="btn btn-primary"onclick="{ viewcarrentalAgentLocation('<%= agent %>') }"> View Location</button></td>
                </tr>
            <% })%>

            </tbody>
        </table>



    </div>

    <div class="col-md-4">
        <h3>Agent Location</h3>
        <div id="map">
        </div>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDNtTpw_MrAdwrPlXGTlVVS5BjH5xZeh50&callback=initMap"
                async defer></script>

    </div>
</div>
<script src="/js/firebase.js"></script>
<script src="/js/database.js"></script>


</body>
</html>