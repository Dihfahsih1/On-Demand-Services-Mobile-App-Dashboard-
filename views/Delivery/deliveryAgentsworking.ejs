<% include ../menu.html %>
            <div class="page-header"><h1> Delivery Agents Working</h1></div>
    <div class="col-md-5">
        <% var keys=Object.keys(workingdeliveryAgents)  %>

        <table class="table" border="2">
            <thead class="thead-dark">
            <tr>
                <th scope="col">Total Online</th>
                <th scope="col"><p><%= size %></p></th>
            </tr>
            <tr>
                <th scope="col">UserName</th>
                <th scope="col">Phone</th>
                <th scope="col" >View Trip</th>
                <th scope="col">View Location</th>
            </tr>
            </thead>
            <tbody>
            <% keys.forEach(function(agent){ %>
                <tr onmouseover="{ viewDeliveryTripLocation('<%= agent %>') }">
                    <td><%= workingdeliveryAgents[agent].agentName %></td>
                    <td><%= workingdeliveryAgents[agent].agentPhone %></td>
                    <td><button class="btn btn-primary"onclick="{ viewDeliveryTrip('<%= agent %>') }"> View Delivery Trip</button></td>
                    <td><button class="btn btn-primary"onclick="{ viewDeliveryTripLocation('<%= agent %>') }"> View Location</button></td>

                </tr>
            <% })%>

            </tbody>
        </table>


    </div>

    <div class="col-md-5" >
        <h3>Trip Details</h3>
        <h5 id="destination"></h5>
        <h5 id="pickup"></h5>
        <h5 id="paymentMode"></h5>
        <h5 id="customer"></h5>

        <h3>Delivery Trip Location</h3>
        <div id="map">
        </div>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDNtTpw_MrAdwrPlXGTlVVS5BjH5xZeh50&callback=initMap"
                async defer></script>
    </div>
</div>
<script src="/js/firebase.js"></script>
<script src="/js/database.js"></script>


</body>
</html>