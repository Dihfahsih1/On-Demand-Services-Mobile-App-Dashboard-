<% include ../menu.html %>
<div class="container-fluid" style="margin-left: 15%;margin-top: -40%">
            <div class="page-header"><h1> Electrician Online</h1></div>
    <div class="col-md-5">
        <% var keys=Object.keys(onlineelectricians)  %>
        <table class="table" border="2">
            <thead class="thead-dark">
            <tr >
                <th >Total Online</th>
                <th ><p><%= size %></p></th>
            </tr>
            <tr>
                <th >UserName</th>
                <th >Phone</th>
                <th  >View Location</th>
            </tr>
            </thead>
            <tbody>
            <% keys.forEach(function(agent){ %>
                <tr onmouseover="{ viewelectricianLocation('<%= agent %>') }">
                    <td><%= onlineelectricians[agent].agentName %></td>
                    <td><%= onlineelectricians[agent].agentPhone %></td>
                    <td><button class="btn btn-primary"onclick="{ viewelectricianLocation('<%= agent %>') }"> View Location</button></td>
                </tr>
            <% })%>
            </tbody>
        </table>
    </div>
    <div class="col-md-4">
        <h3>Agent Location</h3>
        <div id="map">
        </div>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDNtTpw_MrAdwrPlXGTlVVS5BjH5xZeh50&callback=initMap"
                async defer></script>
    </div>
</div>